{{!-- id, first_name, last_name, country, gender, skill, years_experience --}}
<div class="container">
    <h1>Countries:</h1>
    <table>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Country</th>
            <th>Gender</th>
            <th>Skill</th>
            <th>Experience</th>
        {{#each years}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.country}}</td>
            <td>{{this.gender}}</td>
            <td>{{this.skill}}</td>
            <td>{{this.years_experience}} Years</td>
        </tr>
        {{/each}}
    </table>
</div>